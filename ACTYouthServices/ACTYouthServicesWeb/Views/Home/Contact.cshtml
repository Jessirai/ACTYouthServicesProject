<!--display location on google map-->
<div id="googleMap"></div>

<script>
    function myMap() {
        var mapProp = {
            center: new google.maps.LatLng(- 35.249425, 149.143374),
            zoom: 14,
        };
        var myLatLng = { lat: -35.249425, lng: 149.143374 };
        var marker = new google.maps.Marker({
            position: myLatLng,
            map: map,
            title: 'Hello World!'
        });


        marker.setMap(map);
        var map = new google.maps.Map(document.getElementById("googleMap"), mapProp);
        /*plot markers for services*/
        var marker = new google.maps.Marker({
            position: {
                lat: -35.249425,
                lng: 149.143374
            },
            map: map,
            title: 'Northside Community Service'

        });
        var contentString = 'Northside Community Service';
        var infowindow = new google.maps.InfoWindow({
            content: contentString
        });
        marker.addListener('click', function () {
            infowindow.open(map, marker);
        });
    }
</script>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDXq96VjA3ovASTp9u95xIFoNifTgSZWFk&callback=myMap"></script>







